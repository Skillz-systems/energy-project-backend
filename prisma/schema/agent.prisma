model Agent {
    id String @id @default(auto()) @map("_id") @db.ObjectId

    customers Customer[] 

    agentId   Int
   
    user   User   @relation(fields: [userId], references: [id])
    userId String @unique @db.ObjectId

    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    deletedAt DateTime?
}
