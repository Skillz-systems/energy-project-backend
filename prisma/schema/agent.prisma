model Agent {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  customers Customer[]
  sales     Sales[]

  agentId Int

  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId String @unique @db.ObjectId

  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  @@map("agents")
}
